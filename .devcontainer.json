{
    "image": "aws-node:alpine",
    "forwardPorts": [3000, 4200, 5010, 5100, 8080, 15672, 5672],
    "workspaceFolder": "/workspace",
    "mounts": [
        "source=${env:USERPROFILE}/.aws,target=/root/.aws,type=bind,consistency=consistent",
        "source=${env:USERPROFILE}/.ssh,target=/root/.ssh,type=bind,consistency=consistent",
        // "source=${env:USERPROFILE}/.docker,target=/root/.docker,type=bind,consistency=consistent",
        "source=${env:USERPROFILE}/.nuget,target=/root/.nuget,type=bind,consistency=consistent",
        "source=${env:USERPROFILE}/.kube,target=/root/.kube,type=bind,consistency=consistent",
        "source=${env:USERPROFILE}/.bashrc,target=/root/.bashrc,type=bind,consistency=consistent",
        "source=${env:USERPROFILE}/.bash_profile,target=/root/.bash_profile,type=bind,consistency=consistent",
        "source=${env:USERPROFILE}/.bash_aliases,target=/root/.bash_aliases,type=bind,consistency=consistent",
        "source=${env:USERPROFILE}/vscode.gitconfig,target=/root/.gitconfig,type=bind,consistency=consistent",
        "source=${env:APPDATA}/Microsoft/UserSecrets,target=/root/.microsoft/usersecrets,type=bind,consistency=consistent",
        "source=${env:APPDATA}/Code - Insiders/User/settings.json,target=/root/.vscode-server-insiders/data/Machine/settings.json,type=bind,consistency=consistent",
        "source=${env:APPDATA}/Code - Insiders/User/container-extensions,target=/root/.vscode-server-insiders/extensions,type=bind,consistency=consistent",
        "source=${env:APPDATA}/helm/.config,target=/root/.config/helm,type=bind,consistency=consistent",
        "source=${env:APPDATA}/helm/.local,target=/root/.local/share/helm,type=bind,consistency=consistent",
        "source=//var/run/docker.sock,target=/var/run/docker.sock,type=bind,consistency=consistent"
    ],
    "workspaceMount": "source=${localWorkspaceFolder},target=/workspace,type=bind,consistency=consistent",
    "runArgs": ["--rm", "--network=host"],
    "containerEnv": {
        "AWS_PROFILE": "PRB-Tools-East1"
    },
    "extensions": [
        "ms-kubernetes-tools.vscode-kubernetes-tools",
        "zxh404.vscode-proto3",
        "ms-vscode.powershell-preview",
        "aliasadidev.nugetpackagem",
        "nrwl.angular-console",
        "adrianwilczynski.user-secrets",
        "ms-dotnettools.vscode-dotnet-runtime",
        "formulahendry.docker-explorer",
        "ms-azuretools.vscode-docker",
        "ecmel.vscode-html-css",
        "esbenp.prettier-vscode",
        "ms-dotnettools.csharp",
        "amazonwebservices.aws-toolkit-vscode",
        "mark-tucker.aws-cli-configure",
        "christian-kohler.npm-intellisense",
        "dbaeumer.vscode-eslint",
        "foxundermoon.shell-format",
        "oderwat.indent-rainbow",
        "2gua.rainbow-brackets",
        "hashicorp.terraform",
        "erd0s.terraform-autocomplete",
        "redhat.vscode-yaml",
        "arcanis.vscode-zipfs",
        "mindaro.mindaro"
    ]
}
